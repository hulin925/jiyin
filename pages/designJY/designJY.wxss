  overflow: auto;
}
.frameBox{
  padding:20rpx;
}
.frameConten{
  padding:20rpx;
  background-color: #fff;
}
.frameContenT{
  height:70rpx;
  line-height:70rpx;
  border-bottom:1px solid #ededed
}
.frameContenT .icon-cha{
  float:right;
  padding:0 20rpx;
}
.frameInfo{
  padding:10rpx;
}
.framInfoT{
  height:70rpx;
  line-height:70rpx;
  background-color: #023a69;
  color:#fff;
  padding-left:12rpx;
  font-size:36rpx;
}
.framInfoT1{
  height:60rpx;
  line-height:60rpx;
  font-size:36rpx;
}

.table {
  font-size:34rpx;
  text-align:center;
}
.tr {
  height:70rpx;
  line-height:70rpx;
  display: flex;
  width: 100%;
  justify-content: space-around;
  background-color: #e4e4e4;
}
.td {
    width:20%;
    overflow:hidden;
    text-overflow:ellipsis;/* 超出部分显示省略号 */
    white-space: nowrap;/*规定段落中的文本不进行换行 */
}
.th {
  width:20%;
}
.bg-w{
  height:70rpx;
  line-height:70rpx;
  background-color: #023a69;
  color:#fff;
}
.bg-g{
  background: #f5f3f3;
}
.bg-add{
  color:#023a69;
}
.more{
  height:70rpx;
  line-height:70rpx;
  color:#023a69;
  font-size:32rpx;
}

.bodyLock{
    top:0px;
    left: 0px;
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: fixed;
    z-index: 0;
}
.hide{
  display:none;
}
.show{
  display:block;
}

.details{
  color:#1a7bff;
}
.active{
  border:4rpx solid green;
  color:green;
  position:relative;
}
.active::before{
  position:absolute;
  content:'';
  width:60rpx;
  height:60rpx;
  background-image: url('https://web.3fgj.com/imgJY/gou.png');
  background-repeat: no-repeat;
  background-size: cover;
  /* background-position: center center; */
  bottom:-15rpx;
  right:-15rpx;
}


